PROJECT_NAME           = "AIO"
PROJECT_NUMBER         = 0.1.0
PROJECT_BRIEF          = "Low-level, data-oriented asynchronous I/O library for C++20"
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_SOURCE_DIR@/docs/out/
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/include/aio
RECURSIVE              = YES
GENERATE_TREEVIEW      = YES # optional. Also works without treeview
DISABLE_INDEX = NO
FULL_SIDEBAR = NO
HTML_EXTRA_STYLESHEET  = @CMAKE_CURRENT_SOURCE_DIR@/external/doxygen-awesome/doxygen-awesome.css
HIDE_UNDOC_CLASSES     = YES
HIDE_UNDOC_MEMBERS     = NO
HTML_COLORSTYLE        = LIGHT # required with Doxygen >= 1.9.5
SHOW_FILES             = NO
SORT_MEMBER_DOCS       = NO
SORT_MEMBERS_CTORS_1ST = YES
SOURCE_BROWSER         = NO
INLINE_SOURCES         = NO
INLINE_SIMPLE_STRUCTS  = NO
REFERENCED_BY_RELATION = YES
REFERENCES_RELATION    = YES
CLANG_ASSISTED_PARSING = YES
HAVE_DOT               = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
SEPARATE_MEMBER_PAGES   = NO